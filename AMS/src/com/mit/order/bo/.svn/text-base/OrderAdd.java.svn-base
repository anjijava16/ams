package com.mit.order.bo;

import net.sf.json.JSONObject;


/**
 * T_ORDER_ADD 实体类 Wed Nov 02 18:10:19 CST 2016 刘鹏飞
 */

public class OrderAdd {
	private Long id;
	private Long order_id;
	private String add_name;
	private String add_desc;
	private float add_total;
	private String bak;
	private String del_flag;

	public void setId(Long id) {
		this.id = id;
	}

	public Long getId() {
		return id;
	}

	public void setOrder_id(Long order_id) {
		this.order_id = order_id;
	}

	public Long getOrder_id() {
		return order_id;
	}

	public void setAdd_name(String add_name) {
		this.add_name = add_name;
	}

	public String getAdd_name() {
		return add_name;
	}

	public void setAdd_desc(String add_desc) {
		this.add_desc = add_desc;
	}

	public String getAdd_desc() {
		return add_desc;
	}

	public void setAdd_total(float add_total) {
		this.add_total = add_total;
	}

	public float getAdd_total() {
		return add_total;
	}

	public void setBak(String bak) {
		this.bak = bak;
	}

	public String getBak() {
		return bak;
	}

	public void setDel_flag(String del_flag) {
		this.del_flag = del_flag;
	}

	public String getDel_flag() {
		return del_flag;
	}
	public String toString() {
		return JSONObject.fromObject(this).toString();
	}
}
